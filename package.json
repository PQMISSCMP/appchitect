{
  "name": "ms-parametricas",
  "version": "0.1.0",
  "description": "Parametricas de datos.",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "concurrently \"tsc --watch\" \"nodemon dist/index.js\"",
    "start:prod": "npm run build && npm start",
    "build": "tsc",
    "predocker": "docker build -t ms-parametricas .",
    "docker": "docker run --env-file .env -p 3000:3000 ms-parametricas",
    "reinstall": "rm -rf node_modules && rm -rf package-lock.json && npm cache clean --force && npm install",
    "test:coverage": "jest --forceExit --coverage --verbose",
    "posttest": "eslint src/**"
  },
  "private": true,
  "author": "LFE",
  "license": "UNLICENSED",
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/node": "^12.12.8",
    "@types/request": "^2.48.3",
    "@types/request-promise-native": "^1.0.17",
    "@types/swagger-ui-express": "^4.1.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "helmet": "^3.21.2",
    "oracledb": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "swagger-ui-express": "^4.1.2",
    "typeorm": "^0.2.20",
    "typescript": "^3.7.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^6.6.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "standardx": "^5.0.0",
    "ts-jest": "^24.2.0"
  }
}
